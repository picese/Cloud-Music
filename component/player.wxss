/* 播放器 */

.player {
  width: 100%;
  height: 140rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 2;
  background-color: #38b0de;
}

.player navigator{
  display: block;
  width: 70%;
  opacity: 1;
  background-color:none;
}

.player image {
  width: 120rpx;
  height: 120rpx;
  margin: 12rpx;
  display: inline-block;
}

.song {
  display: flex;
  flex-direction: row;
  align-content: center;
  width: 76%;
  line-height: 1.6;
}

.player-songName {
  width: 65%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}

.song-name {
  font-size: 15px;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.lyric {
  font-size: 12px;
  display: inline-block;
}

.player .play-btn {
  margin-top: 42rpx;
  font-size: 30px;
}

.player .list-btn {
  margin-top: 42rpx;
  font-size: 30px;
  padding-left: 10rpx;
}

/* 播放列表 */

.meun {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  border-top: 2rpx solid #ccc;
  padding-bottom: 140rpx;
}

.head {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 20rpx;
  border-bottom: 2rpx solid #ccc;
}

.order {
  display: flex;
  flex-direction: row;
}

.order text {
  font-size: 13px;
  margin-left: 4rpx;
  line-height: 48rpx;
}

.meun .box {
  height: 574rpx;
  overflow-y: scroll;
}

.meun .list {
  display: flex;
  flex-direction: row;
  padding: 0;
  margin: 0;
  position: relative;
  border-bottom: 2rpx solid #ccc;
  margin-left: 40rpx;
  height: 86rpx;
  line-height: 86rpx;
}

.meun .list .name {
  max-width: 50%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  flex-shrink: 2;
  font-size: 15px;
}

.meun .song-msg{
  width: 80%;
}

.meun .list .singer {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  flex-shrink: 0;
  font-size: 12px;
}

.meun .list .icon-yinyueshi {
  margin-left: 30rpx;
}

.meun .list .bofang {
  position: absolute;
  top: 0;
  right: 10%;
}

.meun .list .guanbi {
  position: absolute;
  top: 0;
  right: 0;
}

image {
  width: 46rpx;
  height: 46rpx;
  display: block;
}
